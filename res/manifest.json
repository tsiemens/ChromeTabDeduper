{
   "manifest_version": 2,

   "name": "Tab Deduper",
   "description": "An extension that allows the user to deduplicate tabs.",
   "version": "1.0",
   "icons": {
      "128": "icon.png"
   },

   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "background": {
      "scripts": ["eventPage.js"],
      "persistent": false
   },
   "options_page": "options.html",
   "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "contextMenus"
   ]
}
