{
   "manifest_version": 2,

   "name": "Tab Deduper",
   "description": "This extension allows the user to deduplicate tabs.",
   "version": "1.0",
   "icons": {
      "128": "icon.png"
   },

   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "background": {
      "scripts": ["utils.js", "eventPage.js"],
      "persistent": false
   },
   "permissions": [
      "tabs",
      "activeTab",
      "storage"
   ]
}
